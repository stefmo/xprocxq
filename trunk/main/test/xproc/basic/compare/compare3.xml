 <p:pipeline name="pipeline"
            xmlns:p="http://www.w3.org/ns/xproc"
            xmlns:util = "http://xproc.net/xproc/util">

<util:step name="pipeline">
    <p:input port="source" primary="true"/>
    <p:output port="result" primary="true"/>
</util:step>

    <p:wrap name="wrapstep">
        <p:with-option name="wrapper" value="mytestwrapperelement"/>   
        <p:with-option name="match" value="/"/> 
    </p:wrap>
    
   <p:compare>
        <p:input port="source" primary="true">
            <p:pipe step="source" port="input"/>
        </p:input>
        <p:input port="alternate">
              <p:pipe step="wrapstep" port="result"/>
        </p:input>
        <p:output port="result"/>
   </p:compare>

</p:pipeline>
