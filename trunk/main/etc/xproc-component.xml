<xproc:components xmlns:p ="http://www.w3.org/ns/xproc"
               xmlns:xproc = "http://xproc.net/xproc"
               name="xproc-components">

    <xproc:root-element type="p:pipeline" xproc:step="true"/>

    <xproc:element type="p:pipeline" xproc:step="true"/>

    <xproc:element type="p:library"/>

    <xproc:element type="p:declare-step" xproc:step="true" xproc:input="true" xproc:output="true">
      <p:input port="source"/>
      <p:output port="result"/>
    </xproc:element>

    <xproc:element type="p:import"/>
    <xproc:element type="p:import-schema"/>

<!-- //-->    
    <xproc:element type="p:input"/>
    <xproc:element type="p:output"/>

<!-- //-->    
    <xproc:element type="p:document"/>

<!-- //-->    
    <xproc:element type="p:for-each" xproc:step="true"/>

    <xproc:element type="p:choose" xproc:step="true"/>
    <xproc:element type="p:when"/>            
    <xproc:element type="p:otherwise"/>     

    <xproc:element type="p:viewport" xproc:step="true"/>
    <xproc:element type="p:try"/>
    <xproc:element type="p:catch"/>

<!-- //-->    
    <xproc:element type="p:group" xproc:step="true"/>
    <xproc:element type="p:documentation"/>
    <xproc:element type="p:serialization"/>


    <xproc:element type="p:iteration-source"/>
    <xproc:element type="p:viewport-source"/>
           
    <xproc:element type="p:variable"/>            
    <xproc:element type="p:namespaces"/>            

    <xproc:element type="p:serialization"/>            
    <xproc:element type="p:option"/>            

    <xproc:element type="p:with-option"/>            
    <xproc:element type="p:with-param"/>            

    <xproc:element type="p:pipe"/>
    <xproc:element type="p:empty"/>
    <xproc:element type="p:inline"/>
    <xproc:element type="p:pipeinfo"/>
    
    
    <xproc:element type="p:xpath-context"/>

    <xproc:element type="p:log"/>
    <xproc:element type="p:document"/>
    <xproc:element type="p:file"/>
        
</xproc:components>