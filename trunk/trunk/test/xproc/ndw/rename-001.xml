<t:test xmlns:t="http://xproc.org/ns/testsuite"
        xmlns:p="http://www.w3.org/ns/xproc"
        xmlns:c="http://www.w3.org/ns/xproc-step"
        xmlns:err="http://www.w3.org/ns/xproc-error">

<t:title>Test rename001</t:title>

<t:input port='source'>
<p:pipeline name="pipeline" xmlns:p="http://www.w3.org/2007/03/xproc">
<p:input port="source"/>
<p:output port="result"/>

<p:rename>
  <p:option name="target" value="p:rename"/>
  <p:option name="name" value="p:changed-rename"/>
</p:rename>

</p:pipeline>
</t:input>

<t:pipeline>
<p:pipeline name="pipeline">
<p:input port="source"/>
<p:output port="result"/>

<p:rename>
  <p:option name="target" value="p:rename"/>
  <p:option name="name" value="p:changed-rename"/>
</p:rename>

</p:pipeline>
</t:pipeline>

<t:output port='result'>
<p:pipeline xmlns:p="http://www.w3.org/2007/03/xproc" name="pipeline">
<p:input port="source"/>
<p:output port="result"/>

<p:changed-rename>
  <p:option value="p:rename" name="target"/>
  <p:option value="p:changed-rename" name="name"/>
</p:changed-rename>

</p:pipeline></t:output>

</t:test>
